# Basements
<Floors -1 to -10>
    Set 2 = %floor% * -1
    ID = LL%2%
    Name = Lower Level %2%
    Type = Basement
    Height = 9.5
    InterfloorHeight = 2.24
    if(%floor% = -1) Altitude = -2.24

    #Floor
    FloorOrientation = top
    AddFloor Floor, Brick, 0.5, -75, 70, 75, -70, 0, 0, 0, 0, False

    #Ceiling
    FloorOrientation = bottom
    AddFloor Ceiling, Brick, 0.5, -75, 70, 75, -70, %height% - 0.05, %height% - 0.05, 0, 0, False

    #Interfloor bottom
    FloorOrientation = bottom
    AddInterfloorFloor IFloor, Brick, 0.5, -75, 70, 75, -70, 0, 0, 0, 0

    #Interfloor top
    FloorOrientation = top
    AddInterfloorFloor ITop, Brick, 0.5, -75, 70, 75, -70, %interfloorheight% - 0.05, %interfloorheight% - 0.05, 0, 0

    #Interfloor walls
    WallOrientation = left
    AddInterfloorWall IFront, Brick, 0.5, -75, -70, 75, -70, %interfloorheight%, %interfloorheight%, 0, 0, 0, 0
    AddInterfloorWall ILeft, Brick, 0.5, -75, 70, -75, -70, %interfloorheight%, %interfloorheight%, 0, 0, 0, 0
    WallOrientation = right
    AddInterfloorWall IRight, Brick, 0.5, 75, -70, 75, 70, %interfloorheight%, %interfloorheight%, 0, 0, 0, 0
    AddInterfloorWall IBack, Brick, 0.5, 75, 70, -75, 70, %interfloorheight%, %interfloorheight%, 0, 0, 0, 0

    #Level Walls
    WallOrientation = left
    AddWall Front, Brick, 0.5, -75, -70, 75, -70, %height%, %height%, 0, 0, 0, 0, False
    AddWall Left, Brick, 0.5, -75, 70, -75, -70, %height%, %height%, 0, 0, 0, 0, False
    WallOrientation = right
    AddWall Right, Brick, 0.5, 75, -70, 75, 70, %height%, %height%, 0, 0, 0, 0, False
    AddWall Back, Brick, 0.5, 75, 70, -75, 70, %height%, %height%, 0, 0, 0, 0, False
<EndFloors>

